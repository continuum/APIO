%tr{"data-link" => organization_service_service_version_path(service.organization, service, service.last_version)}
  %td= service.created_at.strftime("%d/%m/%Y")
  %td= service.name
  %td
    %span{ class: 'btn btn-tiny static' }
      = 'R' + service.last_version_number.to_s
  %td= service.last_version.user.name
  %td
    %span{ class: css_class_for_status(service.last_version.status) }
      = t(service.last_version.status)
